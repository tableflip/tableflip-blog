{{!< default}}

{{! This is a page template. A page outputs content just like any other post, and has all the same
    attributes by default, but you can also customise it to behave differently if you prefer. }}

<header id="header">
  <div id="logo">
    <a href="/" title="(╯°□°）╯︵ ┻━┻"><img src="/assets/img/tableflip.min.svg"></a>
  </div>
  <nav>
    <ul>
      <li><a href="#about" title="About"><img src="/assets/img/icon-info.svg"><span>About</span></a></li>
      <li><a href="#portfolio" title="Portfolio"><img src="/assets/img/icon-briefcase.svg"><span>Portfolio</span></a></li>
      <li><a href="http://blog.tableflip.io/" title="Blog"><img src="/assets/img/icon-blog.svg"><span>Blog</span></a></li>
      <li><a href="#contact" title="Contact"><img src="/assets/img/icon-phone.svg"><span>Contact</span></a></li>
    </ul>
  </nav>
</header>
<main id="content" role="main">

    <article class="{{post_class}}">
    {{! Everything inside the #post tags pulls data from the post }}
    {{#post}}

        <h1 class="post-title">{{title}}</h1>

        <section class="post-content">
            {{content}}
        </section>

    {{/post}}
    </article>

</main>
